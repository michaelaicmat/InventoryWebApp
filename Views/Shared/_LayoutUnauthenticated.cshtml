<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />

    <title>@ViewBag.Title - Inventory Web App</title>
    <meta name="viewport" content="width=device-width, initial-scale=.86, shrink-to-fit=no">

    <!-- App favicon -->
    <link rel="shortcut icon" href="~/Content/assets/images/BBLogo.jpg">

    <!-- App css -->
    <link href="/Content/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/assets/css/app-creative.min.css" rel="stylesheet" type="text/css" id="light-style" />
    <link href="/Content/assets/css/app-creative-dark.min.css" rel="stylesheet" type="text/css" id="dark-style" />
</head>
<body class="loading" data-layout="topnav" data-layout-config='{"layoutBoxed":false,"darkMode":false,"showRightSidebarOnStart": false}'>
    <div class="wrapper">
      
            <div class="content">
                @RenderBody()

          

            </div>
        
    </div>

   
                 

    <script src="/Content/assets/js/vendor.min.js"></script>
    <script src="/Content/assets/js/app.min.js"></script>

    <script src="/Content/assets/js/vendor/handlebars.min.js"></script>
    <script src="/Content/assets/js/vendor/typeahead.bundle.min.js"></script>

    <script src="/Content/assets/js/pages/demo.typehead.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            //
            $("#btnClear").click(function () {
                $(this).closest('form').find("input[type=number]").val("0");
            });
        });
    </script>
    @RenderSection("scripts", false)
</body>
</html>